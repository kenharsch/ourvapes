<h1 class = "center">All Parts</h1>
<div class = "center">
	<span id="parts-list">
		<h3>
			<table>
				<% @all_prods.each do |prod| %>
				<tr>
					<td><%= prod.id %>:</td>
					<td><%= prod.type %>:</td>
					<td><%= prod.name %>:</td>
					<td><%= prod.description %>:</td>
					<td><%= button_to "Add", {action: "add", controller: "configuration", conf: @conf_objects, add_type: prod.type.downcase, add_id: prod.id}, class: "btn btn-primary"%><%= prod.type %></td>
				</tr>
				<% end %>
			</table>
		</h3>
	</span>
</div>
<%#= @newprod %>