<h1>Main#search</h1>

<%= button_to "buttonTest", "search", :method => :get %>

<table>
<% @products.each do |product| %>
  <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
    <td class="list_description">
      <dl>
        <dt><%= product.name %></dt>
        <dd><%= truncate(strip_tags(product.description), length: 80) %></dd>
      </dl>
    </td>
    <td class="list_actions">
      <%= button_to 'GoToKit', "config", :method => :get %><br/>
    </td>
  </tr>
<% end %>
</table>

<p>Find me in app/views/main/search.html.erb</p>